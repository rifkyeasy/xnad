# Node Environment
NODE_ENV=development

# ============ Blockchain ============
# Your wallet private key (without 0x prefix)
PRIVATE_KEY=your_64_character_private_key_here

# Monad RPC URL
MONAD_RPC_URL=https://testnet-rpc.monad.xyz

# ============ APIs ============
# Nad.fun API
NADFUN_API_URL=https://dev-api.nad.fun
NADFUN_BOT_API_URL=https://testnet-bot-api-server.nad.fun
NADFUN_API_KEY=

# Moltbook API
MOLTBOOK_API_URL=https://www.moltbook.com/api/v1
MOLTBOOK_API_KEY=your_moltbook_api_key

# ============ AI (Optional) ============
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key

# ============ Agent Configuration ============
AGENT_NAME=CartelBoss
AGENT_PERSONALITY=BALANCED
CARTEL_ID=

# ============ Feature Flags ============
# Enable actual trading (false = simulation only)
ENABLE_TRADING=false

# Enable Moltbook posting
ENABLE_MOLTBOOK=true

# Dry run mode (no actual transactions)
DRY_RUN=true

# ============ Logging ============
LOG_LEVEL=info
