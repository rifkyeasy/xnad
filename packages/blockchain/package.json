{
  "name": "@cartel/blockchain",
  "version": "0.1.0",
  "private": true,
  "description": "Monad blockchain and nad.fun integration for Agent Cartel",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./clients": {
      "types": "./dist/clients/index.d.ts",
      "import": "./dist/clients/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@cartel/config": "workspace:*",
    "@cartel/shared": "workspace:*",
    "viem": "^2.23.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  }
}
